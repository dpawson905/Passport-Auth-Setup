{
  "name": "auth-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "start-dev": "concurrently \"npm run dev-win\" \"npm run post-css\"",
    "dev": "DEBUG=app:* nodemon server.js",
    "dev-win": "set DEBUG=app:* & nodemon server.js",
    "post-css": "postcss public/stylesheets/style.css -o public/stylesheets/style.css -m -u autoprefixer -w --verbose",
    "post-css-min": "postcss public/stylesheets/style.css -o public/stylesheets/style.min.css -m -u autoprefixer cssnano"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "cloudinary": "^1.29.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.6",
    "csurf": "^1.11.0",
    "debug": "~4.3.4",
    "dotenv": "^16.0.0",
    "express": "~4.17.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-sanitizer": "^1.0.6",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "html-to-text": "^8.2.0",
    "http-errors": "~2.0.0",
    "method-override": "^3.0.0",
    "mongoose": "^6.2.10",
    "morgan": "~1.10.0",
    "multer": "^1.4.4",
    "multer-storage-cloudinary": "^4.0.0",
    "node-sass-middleware": "1.0.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.0.0",
    "pug": "3.0.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "create-admin-cli": "^1.0.4",
    "cssnano": "^5.1.7",
    "postcss-cli": "^9.1.0"
  }
}
